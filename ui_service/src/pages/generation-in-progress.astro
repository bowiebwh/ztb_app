
---
import BaseLayout from '@/layouts/BaseLayout.astro'
import CommonHeader from '@/components/common/CommonHeader.vue'
import GenerationProgressPage from '@/components/generation-in-progress/GenerationProgressPage.vue'

const breadcrumbs = [
  { label: '项目列表', href: './project-list.html' },
  { label: '项目详情', href: './project-detail-editor.html' },
  { label: '生成与下载', href: './generate-download-step.html' },
  { label: '生成中' },
]
---

<BaseLayout title="生成中 - 智标助手">
  <CommonHeader client:load breadcrumbs={breadcrumbs} />
  <main class="flex-1 bg-background">
    <GenerationProgressPage client:load />
  </main>
</BaseLayout>
