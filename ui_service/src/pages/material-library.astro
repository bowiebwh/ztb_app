
---
import BaseLayout from '@/layouts/BaseLayout.astro'
import CommonHeader from '@/components/common/CommonHeader.vue'
import MaterialLibraryContent from '@/components/material-library/MaterialLibraryContent.vue'

const breadcrumbs = [
  { label: '项目列表', href: './project-list.html' },
  { label: '项目详情', href: './project-detail-editor.html' },
  { label: '材料库' },
]
---

<BaseLayout title="材料库 - 智标助手">
  <CommonHeader client:load breadcrumbs={breadcrumbs} />
  <main class="flex-1 bg-background">
    <MaterialLibraryContent client:load />
  </main>
</BaseLayout>
